<section class="panel">
    <header class="panel-heading">
        New Entity
    </header>
    <!-- page start-->
    <div class="panel-body">
        {% form_theme form 'bootstrap_4_layout.html.twig' %}
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button class="btn btn-primary"  style="float:right">{{ button_label|default('Save') }}</button>
        {{ form_end(form) }}
    </div>
    <!-- page end-->
</section>
